			<select id="browseSelectInfo" resultMap="simpleBrowse">
			SELECT
			        M.MEM_NAME
			      , R.RESUME_NO 
			      , JF.JOB_NAME
                  , CH.CAR_COM_NAME
		      FROM TBL_RESUME R
			  JOIN TBL_MEMBER M ON (M.MEM_NO = R.IN_MEM_NO)
			  JOIN TBL_CAREER_HISTORY CH ON (R.RESUME_NO = CH.RESUME_NO)
			  JOIN TBL_JOB_FIELD JF ON (R.JOB_NO = JF.JOB_NO)
              GROUP BY R.RESUME_NO, M.MEM_NAME, JF.JOB_NAME, CH.CAR_COM_NAME
              HAVING R.RESUME_NO = #{ onClickResumeNo }
		</select>
		
		<select id="careeaNumber" resultType="_int">
			SELECT
             
				 SUM( ROUND(ABS(MONTHS_BETWEEN(CH.CAR_HIRE_DATE,CH.CAR_ENT_DATE)),0)) as CAREER
			  FROM TBL_RESUME R
			  JOIN TBL_MEMBER M ON (M.MEM_NO = R.IN_MEM_NO)
			  JOIN TBL_CAREER_HISTORY CH ON (R.RESUME_NO = CH.RESUME_NO)
              GROUP BY R.RESUME_NO 
              HAVING R.RESUME_NO = #{ onClickResumeNo }
		</select>		
		
		
		<select id="selectBrowseUsingHistroy" resultMap="myPageScoutList">
			SELECT
		        J.JOB_NAME
		       ,R.RESUME_NO 
		       ,M.MEM_NAME
		       ,R.RESUME_TITLE
		       ,RH.RESUME_BROWSE_KINDS
		       ,RH.RESUME_BROWSE_DATE
		    FROM TBL_RESUME R
		    JOIN TBL_JOB_FIELD J ON (R.JOB_NO = J.JOB_NO)
		    JOIN TBL_MEMBER M ON (R.IN_MEM_NO = M.MEM_NO)
		    JOIN TBL_RESUME_READING_HISTORY RH ON (RH.RESUME_NO = R.RESUME_NO)
		    WHERE CO_MEM_NO = #{ memNo }
		</select>
		
		<select id="brosweHistoryKindsSelect" resultMap="readingHistoryResultMap">
			SELECT
			       RESUME_BROWSE_NO
				 , CO_MEM_NO
				 , RESUME_NO
				 , RESUME_BROWSE_DATE
				 , RESUME_BROWSE_KINDS
			 FROM TBL_RESUME_READING_HISTORY
			WHERE RESUME_NO = #{ onClickResumeNo } 
		</select>
		
		
		<insert id="readingKindsInsert">
			INSERT 
				INTO TBL_RESUME_READING_HISTORY A
				(
				  A.RESUME_BROWSE_NO
				, A.CO_MEM_NO
				, A.RESUME_NO
				, A.RESUME_BROWSE_DATE
				, A.RESUME_BROWSE_KINDS
				)
				VALUES
				(
                 SEQ_RESUME_BROWSE_NO.nextval
                , 2
                , #{ onClickResumeNo }
                , SYSDATE
                , '얕은열람'
				)
		</insert>
		
		<update id="updateTime">
			UPDATE

			   TBL_RESUME_READING_HISTORY
			   SET RESUME_BROWSE_DATE = SYSDATE
			 WHERE RESUME_NO = #{ onClickResumeNo }

		</update>
		
		<select id="selectAllCountDeepOpen" resultType="_int">
			SELECT
					COUNT(1) 
			   FROM TBL_RESUME_READING_HISTORY
			  WHERE RESUME_BROWSE_KINDS ='깊은열람' AND CO_MEM_NO = #{ memNo }
		</select>
		
		<select id="selectAllCountSimpleOpen" resultType="_int">
			SELECT
					COUNT(1) 
			   FROM TBL_RESUME_READING_HISTORY
			  WHERE RESUME_BROWSE_KINDS ='얕은열람' AND CO_MEM_NO = #{ memNo }
		</select>
		
		<select id="selectAllScountNum" resultType="_int">
			SELECT
					COUNT(1) 
			   FROM TBL_RESUME_READING_HISTORY
			  WHERE RESUME_BROWSE_KINDS ='면접제안' AND CO_MEM_NO = #{ memNo }
		</select>
		
		<select id="personalBrosweHistorySelect" resultMap="personalMypageScoutMap">
			SELECT
			     C.CO_COM_NAME
			   , REGEXP_SUBSTR(C.CO_ADDRESS, '[^$]+',1,2) as CO_ADDRESS
			   , R.RESUME_BROWSE_KINDS
			   , C.CO_STATUS
			   , I.SCOUT_KINDS
			   , I.CO_MEM_NO
			FROM TBL_RESUME_READING_HISTORY R
			JOIN TBL_CO_MEMBER_ADD_INFO C ON (R.CO_MEM_NO = C.CO_MEM_NO)
			JOIN TBL_INTERVIEW_PROPOSAL I ON (C.CO_MEM_NO = I.CO_MEM_NO)
		</select>
		

		<select id="selectDetailResume" parameterType="_int" resultMap="detailScoutResumeResultMap">
		SELECT DISTINCT
               R.IN_MEM_NO
		     , M.MEM_NAME
		     , S.SKILLS_NAME
		     , JF.JOB_NAME
		     , CH.CAR_HIRE_DATE
		     , CH.CAR_ENT_DATE
		     , P.POT_KINDS
		     , P.POT_LINK_ADDRESS
		     , P.POT_FILE_ORIGINAL_NAME
		     , SIC.SELF_INTRO_ITEM_CONTENT
		     , CH.CAR_COM_NAME
		     , CH.CAR_DEPT_NAME
		     , CH.CAR_JOB_NAME
		     , CH.CAR_WORK_FIELD
		     , CH.PROJECT_NAME
		     , CH.PROJECT_CONTENT
		     , CH.PROJECT_START_DATE
		     , CH.PROJECT_END_DATE
		     , LH.LICENSE_NAME
		     , LH.LICENSE_AGENCY
		     , LH.LICENSE_DATE
		     , EH.EDU_NAME
		     , EH.EDU_AGENCY
		     , EH.EDU_START_DATE
		     , EH.EDU_END_DATE
		     , EH.EDU_CONTENT
		     , AH.AWD_NAME
		     , AH.AWD_AGENCY
		     , AH.AWD_DATE
		     , AH.AWD_CONTENT
		  FROM TBL_MEMBER M
		  JOIN TBL_RESUME R ON (M.MEM_NO = R.IN_MEM_NO)
		  JOIN TBL_PORTFOLIO P ON (R.RESUME_NO = P.RESUME_NO)
		  JOIN TBL_SELF_INTRODUCTION_CONTENT SIC ON (R.RESUME_NO = SIC.RESUME_NO)
		  INNER JOIN TBL_AWARD_HISTORY AH ON (R.RESUME_NO = AH.RESUME_NO)
		  INNER JOIN TBL_EDUCATION_HISTORY EH ON (R.RESUME_NO = EH.RESUME_NO)
		  INNER JOIN TBL_LICENSE_HISTORY LH ON (R.RESUME_NO = LH.RESUME_NO)
		  INNER JOIN TBL_RESUME_HOLDING_SKILLS RH ON (R.RESUME_NO = RH.RESUME_NO)
          JOIN TBL_SKILLS S ON (S.SKILLS_NO = RH.SKILLS_NO)
		  INNER JOIN TBL_CAREER_HISTORY CH ON (R.RESUME_NO = CH.RESUME_NO)
		  INNER JOIN TBL_JOB_FIELD JF ON (R.JOB_NO = JF.JOB_NO)
		 WHERE M.MEM_NO = 3
		   AND R.RESUME_SCOUT_STATUS = 'Y'
		   AND R.RESUME_NO = #{ resumeNo }
	</select>
	
