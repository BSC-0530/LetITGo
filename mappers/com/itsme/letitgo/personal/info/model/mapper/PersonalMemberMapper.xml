<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itsme.letitgo.personal.info.model.mapper.PersonalMemberMapper">


	<resultMap type="com.itsme.letitgo.personal.info.model.dto.InfoMemberDTO" id="memberResultMap">

		<id property="memNo" column="MEM_NO"/>
		<result property="memId" column="MEM_ID"/>
		<result property="memEmail" column="MEM_EMAIL"/>
		<result property="memPwd" column="MEM_PWD"/>
		<result property="memName" column="MEM_NAME"/>
		<result property="memPhone" column="MEM_PHONE"/>
		<result property="memKinds" column="MEM_KINDS"/>
		<result property="memEnrollDate" column="MEM_ENROLL_DATE"/>
		<result property="memEntYn" column="MEM_ENT_YN"/>
		<result property="memEntDate" column="MEM_ENT_DATE"/>
		<result property="memEntNo" column="MEM_ENT_NO"/>
	</resultMap>
	
	<select id="selectMemberInfo" resultMap="memberResultMap">
		SELECT /* com.itsme.letitgo.member.personal.info.model.mapper.MemberMapper#selectMemberInfo */
			   A.*
		  FROM TBL_MEMBER A
		 WHERE A.MEM_NO = 1 
	</select>
</mapper>